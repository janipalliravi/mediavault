<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Include app databases, prefs, and files in cloud backup -->
        <include domain="database" path="."/>
        <include domain="sharedpref" path="."/>
        <include domain="file" path="."/>
    </cloud-backup>
    <device-transfer>
        <!-- Include same content for device-to-device transfer -->
        <include domain="database" path="."/>
        <include domain="sharedpref" path="."/>
        <include domain="file" path="."/>
    </device-transfer>
</data-extraction-rules>


